import { By } from 'webdriverio';
import chai, { expect } from 'chai';
import allureReporter from '@wdio/allure-reporter'
const switchTransaction = 'android=new UiSelector().resourceId("pl.pkobp.ipkobiznes.dev:id/onOffSwitch'
class Transaction {
    get check_visible_bottom() {
        $(buttonTransaction).waitForDisplayed({ timeout: 5000 });
        $(buttonTransaction).isDisplayed().should.equal(true);
        $(buttonTransaction).click();
        $(switchTransaction).waitForDisplayed({ timeout: 8000 });
        $(switchTransaction).click();
    get assert_switch(){
        chaiExpect(this.switchTransaction.getAttribute("checked").equal(false))
    }
   export default new Transaction()
